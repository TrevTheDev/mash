{
  "mocha.node_options": ["-r","esm"],
  "mocha.requires": [
    "esm",
    "mocha-steps"
  ],
  "mocha.options": {
    "timeout": 600000
  },
  "mocha.files.glob": "src/**/*.test.js",
  "mocha.parallelTests": 5,
